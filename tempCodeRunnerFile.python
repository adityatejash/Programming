def calculate(s):
    a,p = 0,0
    for i in s:
        if i.lower() == 'a':
            a += 1
        elif i.lower() == 'p':
            p += 1
        else:
            continue
    return a,p

def percentage(a,p):
    if a+p == 0:
        return 0
    return (p/(a+p))*100

def main():
    string = "AAPPPPppppA"
    a,p = calculate(string)
    print(f"Attendance: {percentage(a,p):.2f}")
    

main()